{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "updateSwagger",
        "type": "shell",
        "command": "swag",
        "args": [
          "init",
          "--parseDependency",
          "--parseInternal",
          "-g",
          "cmd/main.go"
        ],
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "presentation": {
          "reveal": "always",
          "close": true
        }
      }
    ]
  }